<!DOCTYPE html>
<html>
  <head>
    <title>Vacation Rentals, Homes, Experiences & Places - Heirbnb</title>
    <%= csrf_meta_tags %>
    <%= favicon_link_tag asset_path('heirbnb10.ico') %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <script type="text/javascript">
      window.hero = "<%= image_url('hero.svg') %>";
      window.search_icon = "<%= image_url('search_icon.svg') %>";
      window.footer_icon = "<%= image_url('footer_icon.svg') %>";
    </script>
    <%= yield %>
  </body>
</html>
